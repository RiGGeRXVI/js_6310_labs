{
  "manifest_version": 3,
  "name": "KAI SANS",
  "version": "1.2",
  "description": "Добавляет кнопку для изменения стиля и переключения текста на шрифт Comic Sans",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://abiturientu.kai.ru/*", "https://kai.ru/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}